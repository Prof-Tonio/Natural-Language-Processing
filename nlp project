   "source": [
    "def clean(x):\n",
    "    i = re.sub(\"(@\\S+)|(http\\S+)|(\\n)\" ,\" \",x).lower()\n",
    "    i_words = social_tokenizer.tokenize(i)\n",
    "    i_cleaned = [lemmatizer.lemmatize(w)  for w in i_words if w not in stop_words]\n",
    "    return(i_cleaned)\n",
    "\n",
    "cleaned_x = [clean(comment) for comment in x]\n",
    "\n",
    "y = (tweets_csv['class'] == 1)\n",
    "y.value_counts()\n"
   ]
